{
  "name": "static-website-prototype",
  "version": "1.0.0",
  "description": "GWAC static website prototype",
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.2.0"
  },
  "scripts": {
    "build": "run-s postcss:prod cleancss",
    "build:dev": "run-s postcss:dev cleancss",
    "cleancss": "cleancss css/main.css -o css/main.min.css",
    "postcss:dev": "postcss --verbose css/tailwind.css -o css/main.css",
    "postcss:prod": "postcss --verbose --env production css/tailwind.css -o css/main.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GwacCode/static-website-prototype.git"
  },
