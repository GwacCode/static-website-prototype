{
  "name": "gwac-static-website",
  "version": "1.0.0",
  "description": "GWAC static website",
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.2.0"
  },
  "scripts": {
    "build": "run-s postcss:prod cleancss",
    "cleancss": "cleancss styles/main.css -o styles/main.min.css",
    "postcss": "postcss --verbose styles/tailwind.css -o styles/main.css",
    "postcss:prod": "postcss --verbose --env production styles/tailwind.css -o styles/main.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://umphy@bitbucket.org/umphy/gwac-static-website.git"
  },
